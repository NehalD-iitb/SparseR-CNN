python projects/SparseRCNN/train_net.py --config-file /root/code/detectron2/sparsercnn/configs/mask_rcnn_R_50_FPN_3x.yaml --num-gpus 1 --eval-only MODEL.WEIGHTS /root/data/pretrained_models/detectron2/model_final_f10217.pkl